{
  "name": "vuejs-testing",
  "version": "1.0.0",
  "description": "Doing some testing with JS",
  "main": "index.js",
  "scripts": {
    "test": "jest ; xunit-viewer -r junit.xml -o junit.html",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit ; xunit-viewer -r junit.xml -o junit.html"
  },
  "jest": {
    "verbose": true,
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "keywords": [
    "Vuejs"
  ],
  "author": "diamkil",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.24.0",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "xunit-viewer": "^7.0.1"
  },
  "dependencies": {}
}
